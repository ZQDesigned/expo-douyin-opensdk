{
  "name": "expo-douyin-opensdk",
  "version": "0.2.0",
  "description": "Expo module for douyin opensdk",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build",
    "android",
    "ios",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-douyin-opensdk",
    "ExpoDouyinOpenSDK"
  ],
  "repository": "https://github.com/ZQDesigned/expo-douyin-opensdk",
  "bugs": {
    "url": "https://github.com/ZQDesigned/expo-douyin-opensdk/issues"
  },
  "author": "ZQDesigned <2990918167@QQ.COM> (https://github.com/ZQDesigned)",
  "license": "MIT",
  "homepage": "https://github.com/ZQDesigned/expo-douyin-opensdk#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/react": "~18.3.12",
    "expo": "~52.0.0",
    "expo-module-scripts": "^4.0.2",
    "react-native": "0.76.0",
    "release-it": "^17.10.0",
    "@release-it/conventional-changelog": "^9.0.3"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  }
